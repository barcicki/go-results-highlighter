extends ../../template/layout.jade

block variables
    - base = "../"
    - active = "examples"

block title
    title Go Results Web Component - Go Results Highlighter

block content
    .content
        include ../../template/examples.nav.jade

    article
        .content.docs
            h1 Raw results from <a href="http://www.cgerlach.de/go/macmahon.html">MacMahon</a>

            h3 Input:
            p Saved tournament walllist using MacMahon 3.4
            pre
                code.language-html
                    = "<go-results>\n"
                    include ../../resources/macmahon.raw.txt
                    = "</go-results>"

            h3 Output:
            .wrapper
                go-results
                    include ../../resources/macmahon.raw.txt



            h1 Raw results from <a href="http://vannier.info/jeux/gotournaments/opengotha.htm">OpenGotha</a>

            h3 Input:
            p Exported to EGD format using OpenGotha 3.42. Spaces between columns
            pre
                code.language-html
                    = "<go-results>\n"
                    include ../../resources/opengotha.egd.h9
                    = "</go-results>"

            h3 Output:
            .wrapper
                go-results
                    include ../../resources/opengotha.egd.h9

            h1 Table input

            h3 Input:
            p Table generated by MacMahon

            pre
                code.language-html
                    = "<go-results>\n"
                    include:escape ../../resources/macmahon.html
                    = "\n"
                    = "</go-results>"

            h3 Output:
            .wrapper
                go-results
                    include ../../resources/macmahon.html
