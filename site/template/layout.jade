doctype html
html(lang="en")
    head
        block title
        meta(charset="utf-8")
        meta(name="viewport", content="width=device-width")

        link(href="https://fonts.googleapis.com/css?family=Roboto+Condensed:400,400italic", rel="stylesheet", type="text/css")
        link(href="go-results-highlighter.css", rel="stylesheet", type="text/css")
        link(href="styles/styles.css", rel="stylesheet", type="text/css")
        link(href="components/octicons/octicons/octicons.css", rel="stylesheet", type="text/css")
        script(src="go-results-highlighter.js")
        script(src="scripts/main.js")
    body

        header
            .content
                h1 Go Results Highlighter
                h2 by Artur Barcicki

        nav
            .content
                ul.github
                    li
                        a(href="https://github.com/barcicki/go-results-highlighter")
                            span.octicon.octicon-mark-github
                            span View on GitHub
                    li
                        a(href="https://github.com/barcicki/go-results-highlighter/zipball/master")
                            span.octicon.octicon-cloud-download
                            span Download .zip
                    li
                        a(href="https://github.com/barcicki/go-results-highlighter/tarball/master")
                            span.octicon.octicon-cloud-download
                            span Download .tar.gz
                ul
                    li
                        a(href="/")
                            span.octicon.octicon-home
                            span Home
                    li
                        a(href="/docs")
                            span.octicon.octicon-file-text
                            span Documenation
                    li
                        a(href="/examples")
                            span.octicon.octicon-eye
                            span Examples

        block content
