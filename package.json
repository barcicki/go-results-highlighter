{
  "name": "go-results-highlighter",
  "version": "2.1.1",
  "description": "Improves readability of Go results by highlighting opponents of selected player",
  "scripts": {
    "build-lib": "webpack --config=webpack/lib --mode=production",
    "build-lib:dev": "webpack --config=webpack/lib --mode=development --devtool source-map",
    "build-lib:site": "webpack --config=webpack/lib --mode=development --watch",
    "build-bookmark": "webpack --config=webpack/bookmark --mode=production",
    "build-bookmark:dev": "webpack --config=webpack/bookmark --mode=development",
    "build-bookmark:site": "webpack --config=webpack/bookmark --mode=production --watch",
    "build-site": "webpack --config=webpack/site --mode=production",
    "build-site:dev": "webpack --config=webpack/site --mode=development",
    "clean": "rimraf dist",
    "serve": "webpack-dev-server --config=webpack/site --mode=development",
    "build": "npm-run-all build-lib build-bookmark build-site",
    "start": "npm-run-all -p build-lib:site build-bookmark:site serve",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "preversion": "npm-run-all clean build",
    "commit-site": "gh-pages -d dist -f",
    "prepare-site": "npm-run-all clean build commit-site"
  },
  "author": "Artur Barcicki <barcicki@gmail.com>",
  "homepage": "http://barcicki.github.io/go-results-highlighter",
  "repository": {
    "type": "git",
    "url": "https://github.com/barcicki/go-results-highlighter.git"
  },
  "bugs": {
    "url": "https://github.com/barcicki/go-results-highlighter/issues",
    "email": "barcicki@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "go",
    "igo",
    "baduk",
    "weiqi",
    "results",
    "highlighter",
    "highlight",
    "table",
    "go-results",
    "results-highlighter"
  ],
  "main": "./dist/lib.js",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@primer/octicons": "^19.8.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "highlight.js": "^11.9.0",
    "jasmine-core": "^5.1.1",
    "jstransformer-markdown-it": "^3.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "2.2.1",
    "karma-coveralls": "^2.1.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.1.0",
    "karma-spec-reporter": "^0.0.36",
    "karma-webpack": "^5.0.1",
    "markdown-it": "^14.0.0",
    "mini-css-extract-plugin": "^2.8.0",
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.2",
    "pug-html-loader": "^1.1.5",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.5",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browserslist": "> 0.25%, not dead"
}
